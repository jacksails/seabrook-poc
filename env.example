# Core Next.js Environment Variables
# Copy this file to .env.local and fill in your values

# AI Provider API Keys (for future features)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
PERPLEXITY_API_KEY=your_perplexity_key_here

# Database (for future features)
NEON_DATABASE_URL=your_neon_db_url_here

# Vector Database (for future features)
PINECONE_API_KEY=your_pinecone_key_here
PINECONE_ENVIRONMENT=your_pinecone_env_here

# Error Monitoring
SENTRY_DSN=your_sentry_dsn_here

# Application Settings
NODE_ENV=development 